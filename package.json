    },
    "homepage": "https://github.com/HLTech/judge-d-js",
    "devDependencies": {
        "@types/ejs": "^3.1.2",
        "@types/jest": "^29.5.1",
        "@types/mock-fs": "^4.13.1",
        "@types/yargs": "^17.0.24",
        "factory.ts": "^1.4.1",
        "husky": "^8.0.0",
        "jest": "^29.5.0",
        "mock-fs": "^5.2.0",
        "prettier": "^2.8.8",
        "pretty-quick": "^3.1.0",
        "ts-jest": "^29.1.0",
